#include "main.h"
#include<stdio.h>

void TIM2_IRQHandler()
{
	priint("time Auto relode");
	TIM2->SR  &= ~TIM_SR_UIE;
}
int main()
{
	RCC->APB1ENR |= RCC_APB1ENR_TIM2EN;
	TIM2->CR1    |= TIM_CR1_CEN;
	TIM2->DIER   |= TIM_DIER_UIE;

	TIM2->CNT     = 0;
	TIM2->ARR     =1000000;
	HAL_NVIC_EnableIRQ(TIM2_IRQn);
	while(1);
}
